{
  "type": "kafka",
  "spec": {
    "ioConfig": {
      "type": "kafka",
      "consumerProperties": {
        "bootstrap.servers": "broker-1:9092"
      },
      "topic": "FLIGHTS_SUMMARY_EOS",
      "inputFormat": {
        "type": "json"
      },
      "useEarliestOffset": true
    },
    "tuningConfig": {
      "type": "kafka"
    },
    "dataSchema": {
      "dataSource": "flight_summary_eos",
      "timestampSpec": {
        "column": "TIMEPOSITION",
        "format": "auto"
      },
      "dimensionsSpec": {
        "dimensions": [
          "CALLSIGN",
          "PATH",
          "NEAREST_AIRPORT",
          "NEAREST_REGION",
          "NEAREST_CITY",
          {
            "type": "long",
            "name": "LASTCONTACT"
          },
          {
            "type": "double",
            "name": "LATITUDE"
          },
          {
            "type": "double",
            "name": "LONGITUDE"
          },
          {
            "type": "double",
            "name": "ALTITUDE"
          },
          {
            "type": "double",
            "name": "VELOCITY"
          },
          {
            "type": "double",
            "name": "DISTANCE"
          }
        ]
      },
      "metricsSpec": [
        {
          "type": "count",
          "name": "count"
        }
      ],
      "granularitySpec": {
        "type": "uniform",
        "segmentGranularity": "HOUR",
        "queryGranularity": null,
        "rollup": false,
        "intervals": null
      },
      "transformSpec": {
        "transforms": []
      }
    }
  }
}
