      "timestampSpec": {
        "column": "ORDER_TS",
        "format": "auto"
      },
      "dimensionsSpec": {
        "dimensions": [
          "USER_REWARD",
          "STORE_ZIPCODE"
        ],
        "dimensionExclusions" : [
        ]
      },
      "metricsSpec": [
        {
          "type": "count",
          "name": "COUNT"
        },
        {
          "type" : "thetaSketch",
          "name" : "UNIQUE_CODES",
          "fieldName" : "CODE",
          "size" : "16384"
        }
      ],
      "granularitySpec": {
        "type": "uniform",
        "segmentGranularity": "HOUR",
        "queryGranularity": "minute",
        "rollup": true,
        "intervals": null
      },
      "transformSpec": {
        "transforms": [
        ]
      }
    }
  }
}
