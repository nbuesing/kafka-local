{
  "connector.class": "io.confluent.connect.s3.S3SinkConnector",

  "tasks.max": "1",

  "topics": "s3_topic",

  "partitioner.class": "com.rill.connect.storage.partitioner.HourlyPartitioner",
  "locale" : "en-US",
  "timezone" : "UTC",
 
  "_timestamp.extractor" : "RecordField",
  "_timestamp.field" : "foo",

  "_rotate.interval.ms" : "...",
 
  "format.class" : "io.confluent.connect.s3.format.json.JsonFormat",
  "storage.class": "io.confluent.connect.s3.storage.S3Storage",

  "_store.url" : "_ this is to test against a local s3 complaint server called localstack _",
  "__store.url" : "http://localstack:4566",
  "store.url" : "http://cloudserver:8000",

  "s3.region": "_local_",
  "s3.bucket.name": "foo",


  "s3.part.size": "5242880",
  "flush.size": "3",

  "key.converter": "org.apache.kafka.connect.json.JsonConverter",
  "key.converter.schemas.enable": false,

  "value.converter": "org.apache.kafka.connect.json.JsonConverter",
  "value.converter.schemas.enable": false

}
